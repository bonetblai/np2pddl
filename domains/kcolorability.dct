(so-exists (?F Func)
    (forall (?x) 
        (and (exists (?y) (and (?F ?x ?y) (?K ?y)))
             (forall (?y) (implies (?E ?x ?y)
                                   (not (exists (?z)
                                        (and (?F ?x ?z)
                                             (?F ?y ?z)
                                        ))
                                   )
                            )
             )
        )
    )
)

